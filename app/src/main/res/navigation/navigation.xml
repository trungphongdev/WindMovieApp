        tools:layout="@layout/fragment_my_list_movie" />


    <fragment
        android:id="@+id/dashBroadScreen"
        android:label="@string/movieDetailLabel"
        android:name="com.example.windmoiveapp.ui.fragment.DashBoardScreen"
        tools:layout="@layout/dash_board_screen" />

    <fragment
        android:id="@+id/searchFragment"
        android:label="Search"
        android:name="com.example.windmoiveapp.ui.fragment.SearchMovieFragment" />

    <fragment
        android:id="@+id/accountFragment"
        android:label="Account"
        android:name="com.example.windmoiveapp.ui.fragment.AccountInfoFragment" />

    <fragment
        android:id="@+id/notifyFragment"
        android:label="Account"
        android:name="com.example.windmoiveapp.ui.fragment.NotificationFragment" />


